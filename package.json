{
  "name": "amortizer",
  "version": "1.0.0",
  "description": "A website to help visually compare mortgage payoff scenarios (or other compound interest curves).",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel watchify serve",
    "serve": "browser-sync start --config browsersync.config.js",
    "test": "wdio wdio.config.js",
    "watchify": "watchify --debug -t [ vueify ] -t [ babelify --presets [ es2015 ] ] src/main.js --outfile dist/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mniebling/Amortizer.git"
  },
  "author": "Mike Niebling",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babelify": "7.3.0",
    "browser-sync": "2.18.8",
    "browserify": "14.4.0",
    "chai": "4.0.0",
    "less": "2.7.2",
    "npm-run-all": "4.0.2",
    "plotly.js": "1.27.1",
    "vueify": "9.4.1",
    "watchify": "3.9.0",
    "wdio-mocha-framework": "0.5.10",
    "wdio-selenium-standalone-service": "0.0.8",
    "wdio-spec-reporter": "0.1.0",
    "webdriverio": "4.8.0"
  },
  "dependencies": {
    "lodash.debounce": "4.0.8",
    "moment": "2.18.1",
    "normalize-css": "2.3.1",
    "vue": "2.3.3"
  }
}
